<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:WhyDeployDesktopClient.ViewModels"
             xmlns:avalonia="clr-namespace:SpacedGridControl.Avalonia;assembly=SpacedGridControl.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WhyDeployDesktopClient.Views.HomePageView"
             x:DataType="viewModels:HomePageViewModel">
    <Control.Styles>
        <StyleInclude Source="../Styles/LabelStyles.axaml" />
        <StyleInclude Source="../Styles/BorderStyles.axaml" />
    </Control.Styles>
    <avalonia:SpacedGrid RowDefinitions="Auto,*">
        <Label Content="Home Page" Classes="H1" />
        <Border Grid.Row="1" Classes="NiceBorder" />
    </avalonia:SpacedGrid>
</UserControl>